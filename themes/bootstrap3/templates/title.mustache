<div class="heading">
<h1>{{makeTitle}}</h1>
	<ul class="tags badge">
		<li>{{type}}</li>
	</ul>
	{{#if plugin}}<span class="label label-primary module">{{plugin}}</span>{{/if}}
	{{#if release}}<span class="label label-success release">{{release}}</span>{{/if}}
</div>

<div class="sub-heading">
	{{#if title}}
	{{^if plugin}}
		<code class="module">{{name}}</code>
	{{/if}}
	{{/if}}
	{{#if types}}
	<h2><code>{{{makeTypesString types}}}</code></h2>
	{{/if}}
	<span class="inherits">{{#if inherits}}inherits: <a href="{{urlTo inherits}}">{{inherits}}</a>{{else}}&nbsp;{{/if}}</span>
</div>

<ul class="links">
	{{#links}}
		<li><a href="{{href}}">{{title}}</a></li>
	{{/links}}
	{{#ifValidSource src}}
		<li><a href="{{urlSource src type line}}">source</a></li>
	{{/ifValidSource}}
	{{#if download}}
		<li><a href="{{urlDownload .}}">download</a></li>
	{{/if}}
	{{#if test}}
		<li><a href="{{urlTest .}}">tests</a></li>
	{{/if}}
</ul>
<br />